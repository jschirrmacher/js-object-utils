{
  "name": "js-object-utils",
  "version": "1.2.3",
  "description": "Finds differences between objects and other tools to handle objects in Typescript",
  "license": "Apache License v2.0",
  "repository": {
    "url": "https://github.com/jschirrmacher/object-utils.git"
  },
  "keywords": [
    "object",
    "diff",
    "helper",
    "tools"
  ],
  "types": "./dist/types/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "scripts": {
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "build": "npm-run-all build:clean build:compile build:module",
    "build:clean": "rm -rf ./dist",
    "build:compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:module": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json"
  },
  "author": {
    "name": "Joachim Schirrmacher",
    "email": "joachim.schirrmacher@gmail.com"
  },
  "devDependencies": {
    "@types/node": "18.15.11",
    "eslint": "8.38.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.7",
    "prettier-eslint": "15.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vitest": "^0.29.7"
  }
}
